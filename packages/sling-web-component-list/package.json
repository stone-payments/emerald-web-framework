{
  "name": "sling-web-component-list",
  "version": "0.16.0",
  "description": "Sling List component",
  "author": "Stone Pagamentos",
  "main": "dist/cjs/index.js",
  "jsnext:main": "dist/es/index.js",
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "lint": {
            "enable": true
          },
          "brand": {
            "title": "Sling Web Components - List",
            "description": "List Component",
            "repository": "https://github.com/stone-payments/sling-web"
          }
        }
      }
    ]
  },
  "dependencies": {
    "sling-web-framework": "^0.16.0",
    "sling-web-helpers": "^0.16.0"
  }
}
